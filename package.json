{
  "name": "test",
  "module": "src/main.ts",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run --hot src/main.ts\" \"bun run uno:dev\" \"bun run liveReload\"",
    "liveReload": "bunx beth-stack",
    "start": "bun run uno && bun run src/main.ts",
    "db:push": "bunx drizzle-kit push:sqlite",
    "db:studio": "bunx drizzle-kit studio",
    "db:seed": "bun run src/model/store/seed.ts",
    "uno": "bunx --bun unocss",
    "uno:dev": "bunx --bun unocss --watch",
    "typecheck": "bunx --bun tsc",
    "format:check": "prettier --check .",
    "format": "prettier --write . --list-different",
    "lint": "eslint . --report-unused-disable-directives",
    "lint:fix": "eslint . --report-unused-disable-directives --fix"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@kitajs/ts-html-plugin": "^4.0.0",
    "@total-typescript/ts-reset": "^0.6.0",
    "@unocss/transformer-variant-group": "^0.62.0",
    "better-sqlite3": "^11.0.0",
    "bun-types": "latest",
    "concurrently": "^8.2.1",
    "drizzle-kit": "^0.22.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-turbo": "^2.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-isaacscript": "^3.5.5",
    "eslint-plugin-prettier": "^5.0.0",
    "@types/eslint": "^9.0.0",
    "pino": "^9.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "typescript": "^5.2.2",
    "unocss": "^0.62.0"
  },
  "dependencies": {
    "@bogeychan/elysia-logger": "0.1.2",
    "@elysiajs/cron": "^1.0.0",
    "@elysiajs/static": "^1.0.0",
    "@elysiajs/swagger": "^1.0.0",
    "@iconify-json/logos": "^1.1.37",
    "@iconify-json/lucide": "^1.1.128",
    "@libsql/client": "0.9.0",
    "@lucia-auth/adapter-sqlite": "^3.0.0",
    "@lucia-auth/oauth": "^3.2.0",
    "@t3-oss/env-core": "^0.11.0",
    "@tlscipher/holt": "1.2.0",
    "beth-stack": "0.0.34",
    "drizzle-orm": "^0.33.0",
    "drizzle-typebox": "^0.1.1",
    "elysia": "1.1.7",
    "lucia": "^3.0.0",
    "pino-pretty": "^11.0.0",
    "zod": "^3.22.2"
  }
}
